{
  "name": "Hello World!",
  "version": "1.0",
  "manifest_version": 3,
  "description": "My first Chrome extension",
  "browser_action": {
    "default_icon": {
            "19": "img/icon-19.png",
            "38": "img/icon-38.png"
        },
    "default_popup": "popup.html"
  },
  "icons": {
      "16": "img/icon-16.png",
      "128": "img/icon-128.png"
  },
  "permissions": [
  "contextMenus"
  ],
  "background": {
    "scripts": [
    "js/background.js"
    ]
  }
}